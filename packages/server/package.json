{
  "name": "@dev-spendesk/fab-server",
  "version": "1.0.0-rc.10",
  "description": "NodeJS FAB Server",
  "keywords": [
    "fab",
    "server"
  ],
  "homepage": "https://github.com/spendesk/fab#readme",
  "bugs": {
    "url": "https://github.com/spendesk/fab/issues"
  },
  "repository": "spendesk/fab",
  "license": "MIT",
  "author": "Glen Maddern",
  "files": [
    "lib",
    "esm"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --module commonjs --outDir lib",
    "build:esm": "tsc --module esnext --outDir esm",
    "build:watch": "run-p 'build:{cjs,esm} -- --watch --preserveWatchOutput'",
    "clean": "rm -rf esm lib package-lock.json",
    "prepack": "npm run clean && npm run build",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "dependencies": {
    "@dev-spendesk/fab-cli": "1.0.0-rc.10",
    "@dev-spendesk/fab-core": "1.0.0-rc.10",
    "@dev-spendesk/fab-sandbox-node-vm": "1.0.0-rc.10",
    "@fly/v8env": "^0.54.0",
    "@types/concat-stream": "^1.6.0",
    "@types/express": "^4.17.2",
    "@types/http-proxy": "^1.17.4",
    "@types/node": "^12.12.14",
    "@types/yauzl": "^2.9.1",
    "concat-stream": "^2.0.0",
    "cross-fetch": "^3.0.6",
    "express": "^4.17.1",
    "file-to-sha512": "^0.0.1",
    "fs-extra": "^8.1.0",
    "get-stream": "^5.1.0",
    "http-proxy": "^1.18.1",
    "node-cache": "^5.1.0",
    "readable-stream-node-to-web": "^1.0.1",
    "yauzl": "^2.10.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
