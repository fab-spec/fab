{
  "name": "@fab/cli",
  "version": "0.0.7-alpha.31",
  "description": "The CLI entry-point for the FAB ecosystem",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/fab-spec/fab",
  "bugs": "https://github.com/fab-spec/fab/issues",
  "repository": "fab-spec/fab",
  "license": "MIT",
  "author": "Glen Maddern @geelen",
  "files": [
    "/bin",
    "/lib",
    "/templates",
    "/oclif.manifest.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "fab": "./bin/run"
  },
  "scripts": {
    "build": "yarn build:cjs",
    "build:cjs": "tsc --module commonjs --outDir lib",
    "build:watch": "yarn build:cjs --watch --preserveWatchOutput",
    "clean": "rm -rf esm lib package-lock.json",
    "coverage": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "oclif:generate": "oclif-dev manifest && oclif-dev readme",
    "prepack": "npm run clean && npm run build && npm run oclif:generate",
    "postpack": "rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@fab/core": "0.0.7-alpha.31",
    "@fab/server": "0.0.7-alpha.31",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-inject": "^4.0.1",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-typescript": "^2.1.0",
    "@types/decompress": "^4.2.3",
    "@types/fs-extra": "^8.0.1",
    "@types/jju": "^1.4.1",
    "@types/node": "^10",
    "@types/prettier": "^1.19.0",
    "@types/semver": "^6.2.0",
    "@types/shelljs": "^0.8.6",
    "@types/webpack": "^4.41.5",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.4.3",
    "chalk": "^3.0.0",
    "cli-ux": "^5.4.5",
    "deterministic-zip": "^1.1.0",
    "events": "^3.1.0",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "guid-typescript": "^1.0.9",
    "https-browserify": "^1.0.0",
    "jju": "^1.4.0",
    "json-keys-sort": "^2.0.0",
    "memfs": "^3.0.4",
    "node-eval": "^2.0.0",
    "path-browserify": "^1.0.0",
    "pkg-up": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-bytes": "^5.3.0",
    "process": "^0.11.10",
    "regex-parser": "^2.2.10",
    "rollup": "^1.27.10",
    "rollup-plugin-hypothetical": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "semver": "^7.1.1",
    "shelljs": "^0.8.3",
    "stream-browserify": "^2.0.2",
    "stream-http": "^3.1.0",
    "tslib": "^1",
    "typescript": "^3.7.5",
    "url": "^0.11.0",
    "util": "^0.12.1",
    "vm-browserify": "^1.1.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/capture-console": "^1.0.0",
    "capture-console": "^1.0.1",
    "tmp-promise": "^2.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "98ce34423f906c0a9f66f2f87643e64b841e359a",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fab",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
