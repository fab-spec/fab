{
  "name": "@dev-spendesk/fab-input-nextjs",
  "version": "1.0.0-rc.11",
  "description": "Module to kick off a FAB build from a NextJS project",
  "keywords": [
    "aws",
    "deploy",
    "fab",
    "s3"
  ],
  "homepage": "https://github.com/spendesk/fab#readme",
  "bugs": {
    "url": "https://github.com/spendesk/fab/issues"
  },
  "repository": "spendesk/fab",
  "license": "MIT",
  "author": "Glen Maddern",
  "files": [
    "lib",
    "esm",
    "src",
    "runtime.js",
    "build.js",
    "shims"
  ],
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --module commonjs --outDir lib",
    "build:esm": "tsc --module esnext --outDir esm",
    "build:watch": "run-p 'build:{cjs,esm} -- --watch --preserveWatchOutput'",
    "clean": "rm -rf esm lib package-lock.json",
    "prepack": "npm run clean && npm run build",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "dependencies": {
    "@dev-spendesk/fab-cli": "1.0.0-rc.11",
    "@dev-spendesk/fab-core": "1.0.0-rc.11",
    "@types/node": "^12.12.14",
    "@types/path-to-regexp": "^1.7.0",
    "acorn": "^7.1.0",
    "astring": "^1.4.3",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "md5-dir": "^0.2.0",
    "memfs": "^3.0.4",
    "mock-express-request": "^0.2.2",
    "mock-res": "^0.5.0",
    "path-to-regexp": "^6.1.0",
    "prettier": "^1.19.1",
    "webpack": "^4.41.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
